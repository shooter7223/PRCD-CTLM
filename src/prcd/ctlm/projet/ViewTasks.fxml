<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prcd.ctlm.projet.ViewTasksController">
    <Button text="Accueil" onAction="#goHome" />
    <Label GridPane.rowIndex="0" text="Mes tâches à faire" />
    <GridPane GridPane.rowIndex="1">
      <Button mnemonicParsing="false" text="Dummy" onAction="#dummy" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" text="Task" onAction="#task" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" text="Task" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" text="Task" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" text="Task" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" text="Task" GridPane.columnIndex="2" GridPane.rowIndex="2" />
   </GridPane>
</VBox>
